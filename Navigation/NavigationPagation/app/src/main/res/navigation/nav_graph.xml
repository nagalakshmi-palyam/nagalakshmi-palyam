<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/welcomeFragment2">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.lakshmi.navigationpagation.Fragments.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start">
        <action
            android:id="@+id/action_startFragment_to_gameFragment"
            app:destination="@id/gameFragment" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment"
        android:name="com.lakshmi.navigationpagation.Fragments.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game">
        <action
            android:id="@+id/action_gameFragment_to_endFragment"
            app:destination="@id/endFragment"
            app:popUpTo="@id/startFragment" />
        <argument
            android:name="Nagalakshmi Palyam"
            android:defaultValue="default"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/endFragment"
        android:name="com.lakshmi.navigationpagation.Fragments.EndFragment"
        android:label="fragment_end"
        tools:layout="@layout/fragment_end">
        <action
            android:id="@+id/action_endFragment_to_startFragment"
            app:destination="@id/startFragment"
            app:popUpTo="@id/startFragment" />
        <argument
            android:name="GameName"
            android:defaultValue="default"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment2"
        android:name="com.lakshmi.navigationpagation.Fragments.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome">
        <action
            android:id="@+id/action_welcomeFragment2_to_startFragment"
            app:destination="@id/startFragment" />
    </fragment>
</navigation>